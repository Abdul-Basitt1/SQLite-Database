<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="75dp">

    <View
        android:id="@+id/view"
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:layout_alignEnd="@+id/t1"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="-2dp"
        android:background="#332C2C" />

    <View
        android:id="@+id/view6"
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:layout_alignEnd="@+id/t1"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="-288dp"
        android:layout_marginBottom="1dp"
        android:background="#332C2C" />

    <View
        android:id="@+id/view7"
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:layout_alignEnd="@+id/t1"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="-224dp"
        android:layout_marginBottom="0dp"
        android:background="#332C2C" />

    <View
        android:id="@+id/view8"
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:layout_alignEnd="@+id/t1"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="-151dp"
        android:layout_marginBottom="-1dp"
        android:background="#332C2C" />

    <View
        android:id="@+id/view9"
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:layout_alignEnd="@+id/t1"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="-68dp"
        android:layout_marginBottom="-1dp"
        android:background="#332C2C" />

    <TextView
        android:id="@+id/t1"
        android:layout_width="53dp"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="-1dp"
        android:gravity="center"
        android:text="ID"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="65dp"
        android:layout_height="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_toEndOf="@+id/t1"
        android:layout_toRightOf="@id/t1"
        android:gravity="center"
        android:text="NAME"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="83dp"
        android:layout_height="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_toEndOf="@+id/t2"
        android:layout_toRightOf="@id/t2"
        android:gravity="center"
        android:text="EMAIL"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t4"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_toEndOf="@+id/t3"
        android:layout_toRightOf="@id/t3"
        android:gravity="center"
        android:text="NUMBER"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t5"
        android:layout_width="63dp"
        android:layout_height="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_toEndOf="@+id/t4"
        android:layout_toRightOf="@id/t4"
        android:gravity="center"
        android:text="DEG"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t6"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_toEndOf="@+id/t5"
        android:layout_toRightOf="@id/t5"
        android:gravity="center"
        android:text="Skill"
        android:textSize="15sp"
        android:textStyle="bold" />

</RelativeLayout>